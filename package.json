{
  "name": "eslint-plugin-playlyfe",
  "description": "custom eslint stuff for playlyfe",
  "version": "0.0.0-dummy-version",
  "main": "lib/index.js",
  "scripts": {
    "test": "yarn run check-code && cross-env yarn run testonly -- --coverage",
    "testonly": "cross-env NODE_ENV=test jest",
    "check-code": "yarn run lint",
    "flow": "flow check",
    "flow-typed-update": "flow-typed update",
    "lint": "eslint src",
    "report-coverage": "codecov",
    "build": "rm -rf lib && babel ./src --out-dir ./lib --ignore '**/__tests__/**,**/node_modules/**'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "precommit": "yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayank1791989/eslint-plugin-playlyfe.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "playlyfe"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": "Mayank Agarwal <mayank1791989@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mayank1791989/eslint-plugin-playlyfe/issues"
  },
  "homepage": "https://github.com/Mayank1791989/eslint-plugin-playlyfe#readme",
  "dependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "4.13.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "invariant": "2.2.2",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-playlyfe": "2.0.4",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "codecov": "3.0.0",
    "commitizen": "2.9.6",
    "cross-env": "5.1.1",
    "cz-conventional-changelog": "2.1.0",
    "dedent-js": "1.0.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-find-rules": "3.1.1",
    "eslint-friendly-formatter": "3.0.0",
    "flow-bin": "0.61.0",
    "flow-typed": "2.2.3",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "prettier": "1.9.2",
    "semantic-release": "8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testRegex": "__tests__/.*-test.js$",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/__tests__/test-utils/"
    ]
  }
}
