{
  "name": "eslint-plugin-playlyfe",
  "description": "custom eslint stuff for playlyfe",
  "version": "0.0.0-dummy-version",
  "main": "lib/index.js",
  "scripts": {
    "test": "yarn check-code && cross-env yarn testonly --coverage",
    "testonly": "cross-env NODE_ENV=test jest",
    "check-code": "yarn lint",
    "flow-typed-update": "rm -rf && flow-typed update",
    "lint": "eslint src",
    "report-coverage": "codecov",
    "build": "rm -rf lib && babel ./src --out-dir ./lib --ignore '**/__tests__/**,**/node_modules/**'",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayank1791989/eslint-plugin-playlyfe.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "playlyfe"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": "Mayank Agarwal <mayank1791989@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mayank1791989/eslint-plugin-playlyfe/issues"
  },
  "homepage": "https://github.com/Mayank1791989/eslint-plugin-playlyfe#readme",
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "babel-eslint": "10.0.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.11.1",
    "invariant": "2.2.4",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "babel-jest": "24.8.0",
    "babel-preset-playlyfe": "3.1.0",
    "codecov": "3.5.0",
    "commitizen": "3.1.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "dedent-js": "1.0.1",
    "eslint": "5.11.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-find-rules": "3.3.1",
    "eslint-friendly-formatter": "4.0.1",
    "flow-bin": "0.99.1",
    "flow-typed": "2.5.2",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "prettier": "1.17.1",
    "semantic-release": "15.13.12"
  },
  "peerDependencies": {
    "eslint": "^5.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testRegex": "__tests__/.*-test.js$",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "local_modules"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/__tests__/test-utils/"
    ]
  }
}
